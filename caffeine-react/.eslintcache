5 style={{ color: \"red\" }}>Out of Stock</h5>\r\n            )}\r\n             {qty ? (\r\n               <h6>{state}</h6>        \r\n             ) : (<></>)\r\n              }\r\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\r\n              <Form.Label>\r\n                <b>Quantity</b>\r\n              </Form.Label>\r\n              <Form.Control\r\n                size=\"sm\"\r\n                as=\"select\"\r\n                name=\"qty\"\r\n                onChange={(e) => onChangeInput(e)}\r\n                className=\"qty-drop-down\"\r\n              >\r\n                <option></option>\r\n                {qtyNumberDropDown}\r\n              </Form.Control>\r\n            </Form.Group>\r\n            <Link to=\"/products\">\r\n              <Button\r\n                onClick={() => addProductToCart()}\r\n                variant=\"secondary\"\r\n                size=\"sm\"\r\n                className=\"outline-light\"\r\n              >\r\n                Add to cart{\" \"}\r\n              </Button>\r\n            </Link>\r\n          </Col>\r\n        </Row>\r\n      </Container>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\asims\\Desktop\\SIE-ALHASA\\w08\\d04\\caffeine\\caffeine-react\\src\\pages\\NewProduct.jsx",["96","97","98"],"C:\\Users\\asims\\Desktop\\SIE-ALHASA\\w08\\d04\\caffeine\\caffeine-react\\src\\pages\\Cart.jsx",["99","100","101","102","103","104","105","106","107"],"C:\\Users\\asims\\Desktop\\SIE-ALHASA\\w08\\d04\\caffeine\\caffeine-react\\src\\components\\Footer.jsx",["108","109","110","111","112","113","114"],"C:\\Users\\asims\\Desktop\\SIE-ALHASA\\w08\\d04\\caffeine\\caffeine-react\\src\\pages\\Signup.jsx",["115"],"C:\\Users\\asims\\Desktop\\SIE-ALHASA\\w08\\d04\\caffeine\\caffeine-react\\src\\pages\\Login.jsx",["116"],"C:\\Users\\asims\\Desktop\\SIE-ALHASA\\w08\\d04\\caffeine\\caffeine-react\\src\\pages\\Articles.jsx",["117","118","119"],"C:\\Users\\asims\\Desktop\\SIE-ALHASA\\w08\\d04\\caffeine\\caffeine-react\