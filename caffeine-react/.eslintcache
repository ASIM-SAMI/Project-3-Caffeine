rgot loginCallback={userLogin} />\r\n\r\n\r\n          </Route>\r\n\r\n\r\n          <Route exact path=\"/new-product\">\r\n            <NewProduct setAuth={setAuth}\r\n              auth={auth} />\r\n\r\n          </Route>\r\n\r\n          <Route path=\"/products/:id\">\r\n            <ShowOneProduct selectProduct={selectProduct} />\r\n          </Route>\r\n\r\n          <Route exact path=\"/products\">\r\n            <Products setSelectProduct={setSelectProduct} />\r\n          </Route>\r\n\r\n          <Route path=\"/new-article\">\r\n            <NewArticle setAuth={setAuth}\r\n              auth={auth}/>\r\n          </Route>\r\n\r\n          <Route path=\"/:article_id/article\" >\r\n            <ShowOneArticle selectArtcile={selectArtcile} auth={auth}/>\r\n          </Route>\r\n\r\n          <Route path=\"/articles\" >\r\n            <Articles setSelectArticle={setSelectArticle}/>\r\n          </Route>\r\n\r\n          {/* <Footer /> */}\r\n\r\n        </Router>\r\n      )}\r\n\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\meme\\Desktop\\SIE-ALHASA\\week8\\project3\\caffeine\\caffeine-react\\src\\pages\\Signup.jsx",["78","79","80","81","82"],"import React, { useState, useEffect } from \"react\";\r\nimport { useHistory } from \"react-router-dom\";\r\nimport { Row, Form, Col, Button, Alert } from \"react-bootstrap\";\r\nimport axios from \"axios\";\r\n\r\nexport default function Singup(props) {\r\n  const history = useHistory();\r\n\r\n  const [user, setUser] = useState({}); // user info\r\n  const [register, setRegister] = useState(true); // to show aleart\r\n\r\n  //to add the input inside user\r\n  const onChangeInput = ({ target: { name, value } }) => {\r\n    setUser({ ...user, [name]: value });\r\n  };\r\n  // to add the user info to database\r\n  const onSubmit = (event) => {\r\n    event.preventDefault();\r\n    \r\n    axios\r\n      .post(\"http://localhost:5000/api/user/register\", user)\r\n      .then((res) => {\r\n        const user = res.data.user;\r\n        if (user) {\r\n          history.push(\"/login\");\r\n        } else {\r\n          setTimeout(() => {\r\n            setRegister(false);\r\n          }, 1000);\r\n        }\r\n      })\r\n      .catch((err) => console.log(err));\r\n  };\r\n\r\n  return (\r\n    <>\r\n      {!register && (\r\n        <Alert variant={\"danger\"}>\r\n          The email is already in use. Please change the email\r\n        </Alert>\r\n      )}\r\n      <form>\r\n                <h3>Sign Up</h3>\r\n\r\n                <div className=\"form-group\">\r\n                    <label>Name </label>\r\n                    <input  onChange={(e) => onChangeInput(e)} type=\"name\" className=\"form-control\" name=\"name\" placeholder=\"Enter name\" />\r\n                </div>\r\n\r\n                <div className=\"form-group\">\r\n                    <label>Email address</label>\r\n                    <input  onChange={(e) => onChangeInput(e)} type=\"email\" className=\"form-control\" name=\"email\" placeholder=\"Enter email\" />\r\n                </div>\r\n\r\n                <div className=\"form-group\"> \r\n                    <label>Password</label>\r\n                    <input   onChange={(e) => onChangeInput(e)} type=\"password\"                  name=\"password\" className=\"form-control\" placeholder=\"Enter password\" />\r\n                </div>\r\n\r\n            \r\n\r\n                <button   onClick={(e) => onSubmit(e)}type=\"submit\" className=\"btn btn-primary btn-block\">Submit</button>\r\n                \r\n            </form>\r\n    </>\r\n  );\r\n}\r\n","C:\\Users\\meme\\Desktop\\SIE-ALHASA\\week8\\project3\\caffeine\\caffeine-react\\src\\components\\NavBar.jsx",[],"C:\\Users\\meme\\Desktop\\SIE-ALHASA\\week8\\project3\\caffeine\\caffeine-react\\src\\components\\Footer.jsx",[],"C:\\Users\\meme\\Desktop\\SIE-ALHASA\\week8\\project3\\caffeine\\caffeine-react\\src\\pages\\Login.jsx",["83","84","85","86"],"import axios from \"axios\";\r\